{
  "name": "cubs-site-functions",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write .",
    "seed": "tsx src/scripts/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:setup-users": "sst shell --stage dev tsx src/scripts/setup-test-users.ts",
    "test:cleanup-users": "sst shell --stage dev tsx src/scripts/setup-test-users.ts --cleanup"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.908.0",
    "@aws-sdk/client-dynamodb": "3.908.0",
    "@aws-sdk/client-s3": "3.908.0",
    "@aws-sdk/client-ses": "3.908.0",
    "@aws-sdk/lib-dynamodb": "3.908.0",
    "@aws-sdk/s3-request-presigner": "3.908.0",
    "@hono/zod-validator": "0.7.4",
    "aws-jwt-verify": "5.1.1",
    "electrodb": "3.4.6",
    "hono": "4.9.11",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/aws-lambda": "8.10.155",
    "@types/node": "22.18.10",
    "@vitest/coverage-v8": "3.2.4",
    "aws-sdk-client-mock": "4.1.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
