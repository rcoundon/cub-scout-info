{
  "name": "cubs-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "AWS_PROFILE=personal sst dev --stage dev",
    "build": "AWS_PROFILE=scouts sst build --stage production",
    "deploy:production": "AWS_PROFILE=scouts sst deploy --stage production",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "pnpm -r typecheck",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "sst": "3.17.19",
    "typescript": "5.9.3"
  }
}
